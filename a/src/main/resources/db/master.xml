<databaseChangeLog  
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"  
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">  

    <changeSet  id="1"  author="rob">  
        <createSequence sequenceName="studentSequence" incrementBy="1" startValue="1"></createSequence>  
   </changeSet>
    <changeSet  id="2"  author="rob">  
        <createTable  tableName="student">  
            <column  name="id"  type="int" defaultValueSequenceNext="studentSequence">  
                <constraints  primaryKey="true"  nullable="false"/>  
            </column>  
            <column  name="first_name"  type="varchar2(255)">  
                <constraints  nullable="false"/>  
            </column>  
                        <column  name="last_name"  type="varchar2(255)">  
                <constraints  nullable="false"/>  
            </column> 
		<column  name="active"  type="boolean"                       
			defaultValueBoolean="true"/>  
        </createTable>  
   </changeSet>  
</databaseChangeLog>